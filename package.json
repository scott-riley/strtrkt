{
  "name": "strtrkt",
  "description": "React Starterkit",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/scott-riley/strtrkt.git"
  },
  "homepage": "https://github.com/scott-riley/strtrkt",
  "main": "dist/server.js",
  "scripts": {
    "start": "node --harmony ./dist/server.js",
    "build-server": "node node_modules/webpack/bin/webpack --colors --display-error-details --config configs/webpack.server.js",
    "build-client": "node node_modules/webpack/bin/webpack --colors --display-error-details --config configs/webpack.client.js",
    "build": "node node_modules/concurrently/src/main \"npm run build-server\" \"npm run build-client\"",
    "watch-server": "webpack --watch --display-error-details --config configs/webpack.server-watch.js",
    "watch-server-start": "node node_modules/just-wait --timeout 300 --pattern \"dist/server.js\" && npm run start",
    "watch-client": "webpack-dev-server --config configs/webpack.client-watch.js",
    "watch": "concurrently --kill-others \"npm run watch-server-start\" \"npm run watch-server\" \"npm run watch-client\"",
    "clean": "rm -rf dist/* && rm -rf static/dist/*",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel": "6.5.2",
    "babel-core": "6.6.5",
    "babel-loader": "6.2.4",
    "babel-polyfill": "6.6.1",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.5.0",
    "concurrently": "2.0.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "forever": "0.15.1",
    "history": "1.17.0",
    "html-dist": "^0.2.0",
    "json-loader": "0.5.4",
    "just-wait": "1.0.5",
    "koa": "1.2.0",
    "koa-compress": "^1.0.9",
    "koa-compressor": "^1.0.3",
    "koa-proxy": "0.4.1",
    "koa-static": "2.0.0",
    "lost": "^6.7.1",
    "lost-grid": "^5.1.6",
    "normalize.css": "^3.0.3",
    "postcss-calc": "^5.2.0",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.0",
    "postcss-custom-selector": "^1.0.0",
    "postcss-custom-selectors": "^3.0.0",
    "postcss-loader": "^0.8.0",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "react-router": "2.0.0",
    "react-transmit": "3.1.7",
    "style-loader": "^0.13.0",
    "webpack": "1.12.14",
    "webpack-dev-server": "1.14.1",
    "webpack-postcss-tools": "^1.1.1"
  },
  "devDependencies": {},
  "engines": {
    "node": ">=0.10.32"
  }
}
